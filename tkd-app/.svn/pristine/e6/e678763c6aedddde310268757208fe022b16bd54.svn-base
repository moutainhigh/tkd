package com.lc.zy.ball.app.service.activity.bean;

import com.lc.zy.ball.domain.oa.po.StatiumActivity;
import com.lc.zy.ball.domain.oa.po.StatiumActivityMember;

import java.util.List;

/**
 * Created by sl on 16/8/2.
 */
public class StatiumActivityVo extends StatiumActivity {

    private static final long serialVersionUID = 1L;

    // 活动时间
    private String sActivityTime;

    // 活动状态
    private int sActivityStatus;

    // 活动中文状态
    private String sActivityStatusName;

    // 活动报名人员
    private List<StatiumActivityMember> statiumActivityMemberList;

    // 活动报名人数
    private int sActivityNum;

    public static long getSerialVersionUID() {
        return serialVersionUID;
    }

    public int getsActivityStatus() {
        return sActivityStatus;
    }

    public void setsActivityStatus(int sActivityStatus) {
        this.sActivityStatus = sActivityStatus;
    }

    public String getsActivityStatusName() {
        return sActivityStatusName;
    }

    public void setsActivityStatusName(String sActivityStatusName) {
        this.sActivityStatusName = sActivityStatusName;
    }

    public List<StatiumActivityMember> getStatiumActivityMemberList() {
        return statiumActivityMemberList;
    }

    public void setStatiumActivityMemberList(List<StatiumActivityMember> statiumActivityMemberList) {
        this.statiumActivityMemberList = statiumActivityMemberList;
    }

    public int getsActivityNum() {
        return sActivityNum;
    }

    public void setsActivityNum(int sActivityNum) {
        this.sActivityNum = sActivityNum;
    }

    public String getsActivityTime() {
        return sActivityTime;
    }

    public void setsActivityTime(String sActivityTime) {
        this.sActivityTime = sActivityTime;
    }
}
