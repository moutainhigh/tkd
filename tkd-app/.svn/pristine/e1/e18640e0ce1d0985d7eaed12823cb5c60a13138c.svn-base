package com.lc.zy.ball.app.service.order.bean;

/**
 * 订单评论
 *
 * @author sl
 * @Created 16/9/8
 */
public class OrderCommentVo {
    // 教练头像
    private String coachPhoto;
    // 教练姓名
    private String coachName;
    // 订单课程
    private String className;
    // 教练类型(大课,私教)
    private String coachType;
    // 教练花数
    private Integer coachGrade;
    // 道馆星数
    private Integer statiumGrade;
    // 订单id
    private String orderId;
    // 道馆id
    private String statiumId;
    // 教练id
    private String coachId;
    // 评论类型
    private Integer commentType;

    public Integer getCommentType() {
        return commentType;
    }

    public void setCommentType(Integer commentType) {
        this.commentType = commentType;
    }

    public String getCoachPhoto() {
        return coachPhoto;
    }

    public void setCoachPhoto(String coachPhoto) {
        this.coachPhoto = coachPhoto;
    }

    public String getCoachName() {
        return coachName;
    }

    public void setCoachName(String coachName) {
        this.coachName = coachName;
    }

    public String getClassName() {
        return className;
    }

    public void setClassName(String className) {
        this.className = className;
    }

    public String getCoachType() {
        return coachType;
    }

    public void setCoachType(String coachType) {
        this.coachType = coachType;
    }

    public Integer getCoachGrade() {
        return coachGrade;
    }

    public void setCoachGrade(Integer coachGrade) {
        this.coachGrade = coachGrade;
    }

    public Integer getStatiumGrade() {
        return statiumGrade;
    }

    public void setStatiumGrade(Integer statiumGrade) {
        this.statiumGrade = statiumGrade;
    }

    public String getOrderId() {
        return orderId;
    }

    public void setOrderId(String orderId) {
        this.orderId = orderId;
    }

    public String getStatiumId() {
        return statiumId;
    }

    public void setStatiumId(String statiumId) {
        this.statiumId = statiumId;
    }

    public String getCoachId() {
        return coachId;
    }

    public void setCoachId(String coachId) {
        this.coachId = coachId;
    }
}
