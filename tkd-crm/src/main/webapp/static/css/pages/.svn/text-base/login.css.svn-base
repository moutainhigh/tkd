*{padding:0; margin:0;}
input{outline:none}
body,html{width:100%; height:100%;}
a{text-decoration:none;}
body{background-color:rgb(242,242,242);overflow: hidden;}
.loginBody{background-color: rgb(242,242,242);width:100%;}
.loginHeader,.loginFooter{background-color:rgb(33,153,215);height:80px;text-align: center;min-width: 100%;position: fixed;width: 100%;}

.loginCont{width: 1250px;margin:0 auto;height: 500px;padding:0;transition:padding 1s;-moz-transition:padding 1s;-webkit-transition:padding 1s;-o-transition:padding 1s;}
.loginCont>div{display:inline-block;}
.loginCont.left{float:left;}
.loginCont>.right{float:right; width:500px; height:400px; background: rgba(255, 255, 255, 1) 0 0 no-repeat;margin-top: 50px;}

.loginCont>.right dl{padding:30px;}
.loginCont>.right dl dt{border-bottom:1px solid #2199d7; font-size:30px; color:#2199d7; line-height:60px; text-align:center; font-family:"microsoft yahei";}
.loginCont>.right dl dd.ddName{overflow:hidden; margin-top:25px; height:58px; background:#fff; border-radius:10px;}
.loginCont>.right dl dd.ddName em{float:left; width:22px; height:24px; margin:18px 15px 0 25px; background:url(../../../static/img/yonghu.png) 0 0 no-repeat;}
.loginCont>.right dl dd.ddName input{float:left; border:none; color:#ccc; height:48px; padding:5px; width:345px; font-size:18px; color:#333;}

.loginCont>.right dl dd.ddPassword{overflow:hidden; margin-top:25px; height:58px; background:#fff; border-radius:10px;}
.loginCont>.right dl dd.ddPassword em{float:left; width:22px; height:24px; margin:18px 15px 0 25px; background:url(../../../static/img/mima.png) 0 0 no-repeat;}
.loginCont>.right dl dd.ddPassword input{float:left; border:none; color:#333; height:48px; padding:5px; width:345px; font-size:18px;}

.loginCont>.right dl dd.ddTip{overflow:hidden; line-height:20px; margin-left: 60px}
.loginCont>.right dl dd.ddTip span{float:left; color:red; font-size:12px;}
.loginCont>.right dl dd.ddTip a{float:right; font-size:20px; color:#fff; font-family:"microsoft yahei";}

.loginCont>.right dl dd.ddLoginBtn{height:58px;margin-top:30px;}
.loginCont>.right dl dd.ddLoginBtn a{display:block; line-height:58px; text-align:center; color:#fff; font-size:28px; background-color:#2199d7; border-radius:10px;}
.loginCont>.right dl dd.ddLoginBtn a:hover{background-color:#1b6aa5;}
.loginCont>.right dl dd.ddRdes{text-align:center; color:#fff; font-size:16px; font-family:"microsoft yahei"; line-height:50px; margin-top:10px;} 
.loginCont>.right dl dd.ddReg{}
.loginCont>.right dl dd.ddReg a{display:block; height:56px; border:1px solid #fff; border-radius:10px; line-height:56px; text-align:center; color:#fff; font-size:22px;}
.loginCont>.right dl dd.ddReg a:hover{color:#00c7c8; border:1px solid #00c7c8;}
.loginCont>.right dl dd.ddDes{text-align:center; color:#bfbfbf; font-size:18px; font-family:"microsoft yahei"; margin-top:30px;}
.loginCont>.right dl dd.ddDesTel{text-align:center; color:#bfbfbf; font-size:18px; font-family:"microsoft yahei"; margin-top:10px;}
.loginCont>.right dl dd.ddDesTel span a{font-size:12px; margin-right:20px; color:#fff;}



.loginFooter{bottom: 0;}
.loginFooter>span.copy{height:80px;line-height:80px;color:#fff;font-size:16px;font-family: 仿宋 微软雅黑 宋体;}
/*.loginFooter>span.hotphone{height: 80px;line-height: 30px;color: #fff;font-size: 16px;font-family: 仿宋 微软雅黑 宋体;position: relative;top: 12px;width: 180px;display: inline-block;}
.loginFooter>span.hotphone>label{display: block;width: 100%;}*/

@media screen and (max-width: 1300px) {
	.loginBody{text-align: center;}
    .loginCont{
    	width:500px;
    	text-align:center;
    }
    .loginCont>.left{
    	display:none;
    	float:none;
    }
    .loginCont>.right{
    	float:none;
    	display:block;
    }
}









